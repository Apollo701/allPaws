<h2>Dogs Looking for Homes:</h2>
      <div class="row">
        <div class="col s12 m7">
          <div class="card">
            <div class="card-image">
              <img src="/dogs/dog1.jpg">
              <span class="card-title">Card Title</span>
            </div>
            <div class="card-content">
              <p>I am a very simple card. I am good at containing small bits of information.
              I am convenient because I require little markup to use effectively.</p>
            </div>
            <div class="card-action">
              <a href="#">This is a link</a>
              <a href='#'>This is a link</a>
            </div>
          </div>
        </div>
      </div>

<table>
  <thead>
    <tr>
        <th data-field="name">Name</th>
        <th data-field="age">Age</th>
        <th data-field="breed">Breed</th>
        <th data-field="bio">Bio</th>
        <th data-field="sex">Sex</th>
        <th data-field="size">Size</th>
        <th data-field="trained">Trained</th>
        <th data-field="animal_friendly">Animal Friendly</th>
        <th data-field="child_friendly">Child Friendly</th>
        <th data-field="spayed_or_neutered">Spayed or Neutered</th>
    </tr>
  </thead>

  <tbody>
  {{#each dog in model}}
    <tr>
      {{#link-to 'dogs.show' dog}}
        <td>{{dog.name}}</td>
      {{/link-to}}
        <td>{{dog.age}}</td>
        <td>{{dog.breed}}</td>
        <td>{{dog.bio}}</td>
        <td>{{dog.sex}}</td>
        <td>{{dog.size}}</td>
        <td>{{dog.trained}}</td>
        <td>{{dog.animalFriendly}}</td>
        <td>{{dog.childFriendly}}</td>
        <td>{{dog.spayedOrNeutered}}</td>

    </tr>
  {{/each}}
  </tbody>
</table>